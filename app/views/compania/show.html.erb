<%- model_class = Companium -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:nombre_c) %>:</strong></dt>
  <dd><%= @companium.nombre_c %></dd>
  <dt><strong><%= model_class.human_attribute_name(:cuenta) %>:</strong></dt>
  <dd><%= @companium.cuenta %></dd>
  <dt><strong><%= model_class.human_attribute_name(:twi) %>:</strong></dt>
  <dd><%= @companium.twi %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              compania_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_companium_path(@companium), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              companium_path(@companium),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
